
package runnable1;
class two implements Runnable
{
    public void run()
    {
        int i=1;
        while(true)
        {
            System.out.println(i+"java");
            i++;
        }
    }
    
}
public class Runnable1 {


    public static void main(String[] args) {
        two t=new two();
        Thread th=new Thread(t);
        
        th.start();
        int i=1;
        while(true)
        {
            System.out.println(i+"programming");
            i++;
        }
      
    }
    
}
