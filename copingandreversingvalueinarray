
package copingandreverseingarray;


public class Copingandreverseingarray {

    
    public static void main(String[] args) {
         int a[]={11,22,33,44,55,66,77,88};
         int b[]=new int[8];
              for (int x:a)
             System.out.print(x+" ");
              System.out.println("");
         for(int i=a.length-1,j=0;i>=0;i--,j++)
         {
          b[j]=a[i]; 
         }
         for (int x:b)
             System.out.print(x+" ");
    }
    
}
