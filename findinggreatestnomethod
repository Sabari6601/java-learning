package methods2;

public class Methods2 {
 static int max(int n[])
 {
     int max=0;
     for (int i=0;i<n.length;i++)
     {
       if(n[i]>max)
       {
           max=n[i];
       }
       
     }
 return max;
 } 

    public static void main(String[] args) {
        int a[]={1,45,67,89,15,9};
        int value = max(a);
System.out.println(value);
    }
    
}
