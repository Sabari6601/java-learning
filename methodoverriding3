
package methodoverriding3;
class i20
{
        public void company()
    {
        System.out.println("hyundai");
    }
    public void price()
    {
        System.out.println("$12000");
    }
    public void capacity()
    {
        System.out.println("5 seater");
    }
}
class i20nline extends i20
{
   public void price()
   {
       System.out.println("$14000");
   }
   public void roof()
   {
       System.out.println("there is an sunroof");
   }
}
public class Methodoverriding3 {

 
    public static void main(String[] args) {
     i20 black =new i20nline();
       black.company();
     black.capacity();
     black.price();
     
 
 
    }
    
}
