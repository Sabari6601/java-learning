
package primeno1;


public class Primeno1 {

   
    public static void main(String[] args) {
        int a=1249;
        boolean prime=true;
        for (int i=2;i<=Math.sqrt(a);i++)
        {  
            if(a%i==0)
            {
                prime=false;
            }
        }
        if(prime==true)
        {
            System.out.println(a+" :is an prime number");
        }
        
    }
    
}
