package whileloop5;

/**
 *
 * @author 91936
 */
public class Whileloop5 {

    
    public static void main(String[] args) {
        int a=123;
        int r;
        int sum=0;
        while(a>0)
        {
          r=a%10;
          sum=sum+r;
          a=a/10;
        }
        System.out.println(sum);
    }
    
}