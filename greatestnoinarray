
package array2c;

public class Array2c {

    public static void main(String[] args) {
    int a[]={10,11,12,13,77,14,15,16,17,1};
    int max=a[0];
    for (int i=0;i<a.length;i++)
    {
        if(a[i] > max )
        {
            max=a[i];
        
        }
  
    }
      System.out.println("the greatest number is: "+max);
    }
    
}
