
public class Methods {
    static void sabari(int c[],int d[],int g){
     c[0]=70;
     d[4]=c[4];
     g=99;
     System.out.println(c[0]+" "+d[4]+" "+g );
    }

    public static void main(String[] args) {
        int a[]={10,20,30,40,50};
        int b[]=new int[5];
        int p=5;
          System.out.println(a[0]+","+b[4]+" "+p);
        sabari(a,b,p);
        System.out.println(a[0]+","+b[4]+" "+p);
     
    }
    
}
