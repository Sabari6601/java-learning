package whileloop6;

/**
 *
 * @author 91936
 */
public class Whileloop6 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
    int a=125;
        int r;
        int fact=1;
        while(a>0)
        {
          r=a%10;
          fact=fact*r;
          a=a/10;
        }
        System.out.println(fact);
    }
    
}
