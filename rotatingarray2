public class Array3aa {

    public static void main(String[] args) {
      int a[]={55,66,77,88,99,10};
      for (int x:a)
      {
          System.out.print(x+"  ");
      }
          System.out.println(" ");
      
      
      int t=a[a.length-1];
      for (int i=a.length-1;i>0;i--)
      {
          a[i] = a[i-1];
      }
      a[0]=t;
      for (int x:a)
      {
          System.out.print(x+ " ");
      }

    }
    
}
