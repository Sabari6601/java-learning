package array2d1;


public class Array2d1 {

    
    public static void main(String[] args) {
   int a[][]={{1,2,3,4},{10,20,30,40,50,60},{11,22,33,44,55,66,77,88,99}};
   for (int i=0;i<a.length;i++)
   {
       for (int j=0;j<a[i].length;j++)
       {
           System.out.print(a[i][j]+", ");
       }
       
   }
   System.out.println(" ");
   for (int x[]:a)
   {
       for (int y:x)
       {
           System.out.print(y+" ,");
       }
   }
    }
    
}
