
package genericarray;
class one<T>
{
    T a[]=(T[]) new Object[10];
    int length=0;
    
    public void append(T v)
    {
    a[length++]=v;    
    }
    public void display()
    {
        for(int i=0;i<length;i++)
        {
            System.out.println(a[i]);
        }
    }
    
}

public class Genericarray {

    
    public static void main(String[] args) {
        one<String> s=new one<>();
        s.append("hi");
        s.append("hello");
        s.display();
    }
    
}
